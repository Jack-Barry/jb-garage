{
  "projectType": "library",
  "root": "vuetify-components",
  "sourceRoot": "vuetify-components",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/vuetify-components/**/*.{vue,ts,js}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/vuetify-components"],
      "dependsOn": [
        {
          "target": "lint",
          "projects": "self"
        }
      ],
      "options": {
        "jestConfig": "libs/vuetify-components/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["libs/vuetify-components/dist"],
      "dependsOn": [
        {
          "target": "lint",
          "projects": "self"
        }
      ],
      "options": {
        "cwd": "libs/vuetify-components",
        "command": "npx rollup -c"
      }
    }
  }
}
